{
  "source": {
    "includePattern": "\\.(jsx|js|ts|tsx)$",
    "include": [
      "."
    ],
    "exclude": [
      "node_modules","dist","out"
    ]
  },
  "recurseDepth": 10,
  "opts": {
    "destination": "./out",
    "recurse": true,
    "template": "node_modules/better-docs"
  },
  "tags": {
    "allowUnknownTags": [
      "optional"
    ]
  },
  "plugins": [
    "node_modules/better-docs/typescript"
  ]
}